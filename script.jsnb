      "code": "// Debug and troubleshooting for EmotionCam\n// Run this cell if you're getting fallback mode\n\nconsole.log('=== EmotionCam Diagnostic ===');\n\n// Check if face-api is loaded\nif (typeof faceapi === 'undefined') {\n    scrib.show('‚ùå face-api.js is not loaded. Please run the first cell again.');\n} else {\n    scrib.show('‚úÖ face-api.js is loaded');\n    \n    // Check available networks\n    const availableNets = Object.keys(faceapi.nets);\n    scrib.show('Available networks:', availableNets);\n    \n    // Check if models are loaded\n    const modelStatus = {\n        'TinyFaceDetector': faceapi.nets.tinyFaceDetector?.isLoaded || false,\n        'FaceLandmark68Net': faceapi.nets.faceLandmark68Net?.isLoaded || false,\n        'FaceExpressionNet': faceapi.nets.faceExpressionNet?.isLoaded || false\n    };\n    \n    scrib.show('Model loading status:', modelStatus);\n    \n    // Test model loading manually\n    async function testModelLoading() {\n        const testUrls = [\n            'https://cdn.jsdelivr.net/npm/face-api.js@0.22.2/weights',\n            'https://raw.githubusercontent.com/justadudewhohacks/face-api.js/master/weights'\n        ];\n        \n        for (const url of testUrls) {\n            try {\n                scrib.show(`üîÑ Testing model URL: ${url}`);\n                \n                // Test if we can fetch a model file\n                const testResponse = await fetch(`${url}/tiny_face_detector_model-weights_manifest.json`);\n                if (testResponse.ok) {\n                    scrib.show(`‚úÖ Model URL accessible: ${url}`);\n                    \n                    // Try loading one model as a test\n                    try {\n                        await faceapi.nets.tinyFaceDetector.loadFromUri(url);\n                        scrib.show(`‚úÖ Successfully loaded TinyFaceDetector from: ${url}`);\n                        return url; // Return working URL\n                    } catch (loadError) {\n                        scrib.show(`‚ùå Failed to load model: ${loadError.message}`);\n                    }\n                } else {\n                    scrib.show(`‚ùå Model URL not accessible: ${url} (Status: ${testResponse.status})`);\n                }\n            } catch (error) {\n                scrib.show(`‚ùå Network error for ${url}: ${error.message}`);\n            }\n        }\n        return null;\n    }\n    \n    // Run the test\n    testModelLoading().then(workingUrl => {\n        if (workingUrl) {\n            scrib.show(`‚úÖ Found working model URL: ${workingUrl}`);\n            scrib.show('üí° Try restarting the camera - models should work now!');\n        } else {\n            scrib.show('‚ùå No working model URLs found.');\n            scrib.show('üí° Possible causes:');\n            scrib.show('   - Network connectivity issues');\n            scrib.show('   - CDN servers temporarily down');\n            scrib.show('   - CORS policy blocking model downloads');\n            scrib.show('   - Browser security settings');\n        }\n    });\n}\n\n// Check EmotionCam instance\nif (window.emotionCam) {\n    scrib.show('‚úÖ EmotionCam instance exists');\n    scrib.show('Models loaded:', window.emotionCam.modelsLoaded);\n    scrib.show('Camera running:', window.emotionCam.isRunning);\n} else {\n    scrib.show('‚ùå EmotionCam instance not found. Please run the main logic cell.');\n}\n\nscrib.show('üîß Diagnostic complete. Check the results above.');\nscrib.show('üí° If models are failing, try using a VPN or different network.');",
